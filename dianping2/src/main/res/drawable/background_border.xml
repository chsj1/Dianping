<?xml version="1.0" encoding="utf-8"?>
<inset
  xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item android:state_pressed="true">
            <shape>
                <solid android:color="@color/click_gray" />
                <stroke android:width="2.0px" android:color="@color/background_gray" />
                <padding android:left="2.0px" android:top="2.0px" android:right="2.0px" android:bottom="2.0px" />
            </shape>
        </item>
        <item>
            <shape>
                <solid android:color="@color/white" />
                <stroke android:width="2.0px" android:color="@color/background_gray" />
                <padding android:left="2.0px" android:top="2.0px" android:right="2.0px" android:bottom="2.0px" />
            </shape>
        </item>
    </selector>
</inset>